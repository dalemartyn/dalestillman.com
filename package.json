{
  "name": "dalestillman.com",
  "version": "1.0.0",
  "dependencies": {
    "dart-sass": "^1.25.0",
    "focus-visible": "^5.1.0",
    "lodash": "^4.17.15",
    "rellax": "^1.12.1"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@sindresorhus/slugify": "^1.0.0",
    "camelcase": "^6.0.0",
    "dotenv": "^8.2.0",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-changed": "^4.0.2",
    "gulp-imagemin": "^7.1.0",
    "gulp-livereload": "^4.0.2",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-touch-cmd": "0.0.1",
    "gulp-uglify": "^3.0.2",
    "image-size": "^0.8.3",
    "imagemin": "^7.0.1",
    "imagemin-pngquant": "^9.0.0",
    "imagemin-svgo": "^8.0.0",
    "luxon": "^1.24.1",
    "mathsass": "^0.11.0",
    "modularscale-sass": "^3.0.10",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "rev-del": "^2.0.0",
    "rollup": "^2.20.0",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-terser": "^6.1.0",
    "sass-material-colors": "0.0.5",
    "sass-mq": "^5.0.1",
    "sharp": "^0.25.4",
    "stylelint": "^13.6.1",
    "stylelint-scss": "^3.18.0",
    "superstatic": "^6.0.4",
    "svelte": "^3.49.0",
    "twitter": "^1.7.1",
    "twitter-text": "^3.1.0"
  },
  "scripts": {
    "build": "run-s gulp:build eleventy:build",
    "dev": "run-p gulp:dev eleventy:watch serve",
    "dev:sync": "run-p gulp:dev eleventy:serve",
    "gulp:build": "npx gulp build",
    "gulp:dev": "npx gulp",
    "eleventy:build": "ELEVENTY_ENV=production npx @11ty/eleventy",
    "eleventy:serve": "ELEVENTY_ENV=development npx @11ty/eleventy --serve",
    "eleventy:watch": "ELEVENTY_ENV=development npx @11ty/eleventy --watch",
    "serve": "superstatic dist --port 8080 --host 0.0.0.0",
    "s3sync": "run-s s3sync:img s3sync:data",
    "s3sync:img": "aws s3 sync dist/img/ s3://dalestillmancom/v1/ --cache-control max-age=31536000 --exclude \"*\" --include \"*.webp\" --include \"*.png\" --include \"*.svg\"",
    "s3sync:data": "aws s3 sync dist/img/ s3://dalestillmancom/v1/ --exclude \"*\" --include \"*.json\"",
    "s3upload:img": "aws s3 cp dist/img/ s3://dalestillmancom/v1/ --cache-control max-age=31536000 --exclude \"*\" --include \"*.webp\" --include \"*.png\"  --include \"*.svg\" --recursive",
    "s3upload:data": "aws s3 cp dist/img/ s3://dalestillmancom/v1/ --exclude \"*\" --include \"*.json\" --recursive",
    "add_figma_img": "node tasks/images/add-figma-image.js",
    "add_local_img": "node tasks/images/add-local-image.js",
    "instagram": "gulp instagram",
    "twitter": "gulp twitter"
  }
}
